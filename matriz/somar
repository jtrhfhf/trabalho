<?php
$array1 = array(array(1, 2, 3), array(4, 5, 6));
$array2 = array(array(7, 8, 9), array(10, 11, 12));

$somarArray = array();


for ($i = 0; $i < count($array1); $i++){
    for($j = 0; $j < count($array1[$i]); $j++){
        $somarArray[$i][$j] = $array1[$i][$j]+$array2[$i][$j];
    }
}
print_r($somarArray);
?>